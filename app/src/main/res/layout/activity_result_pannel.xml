<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.ResultPannelActivity">

    <include layout="@layout/common_toolbar" />

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/ll_below_start_test"
        android:layout_marginTop="?attr/actionBarSize">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tv_score_card"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_20dp"
                android:layout_marginTop="@dimen/margin_10"
                android:text="@string/scoreCard"
                android:textColor="@color/md_black_1000"
                android:textSize="@dimen/text_size_20"
                android:textStyle="bold" />


            <android.support.v7.widget.CardView
                android:id="@+id/cardView_top1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_score_card"
                android:layout_marginLeft="@dimen/margin_10"
                android:layout_marginRight="@dimen/margin_10"
                android:background="@color/white"
                app:cardCornerRadius="@dimen/spacing_5dp"
                app:cardElevation="3dp"
                app:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white">

                    <android.support.v7.widget.AppCompatImageView
                        android:id="@+id/applogo1"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/margin_10"
                        android:src="@drawable/avision" />

                    <android.support.v7.widget.AppCompatTextView
                        android:id="@+id/tv_result_test_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/applogo1"
                        android:layout_centerHorizontal="true"
                        android:textColor="@color/md_black_1000"
                        android:textSize="@dimen/text_size_16" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_result_test_name"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:id="@+id/rl_result_user"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/margin_10">

                            <ImageView
                                android:id="@+id/iv_result_user"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                app:srcCompat="@drawable/ic_result_user"
                                tools:ignore="ContentDescription" />

                            <LinearLayout
                                android:id="@+id/ll_result_user"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/margin_10"
                                android:layout_toRightOf="@+id/iv_result_user"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/name"
                                    android:textSize="@dimen/text_size_14" />

                                <TextView
                                    android:id="@+id/tv_result_name"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/md_black_1000"
                                    android:textSize="@dimen/text_size_14"
                                    android:textStyle="bold" />
                            </LinearLayout>
                        </RelativeLayout>

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/spacing_5dp"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:layout_marginRight="@dimen/margin_10"
                            android:layerType="software"
                            android:src="@drawable/dotted_drawable"
                            tools:ignore="ContentDescription" />

                        <RelativeLayout
                            android:id="@+id/rl_result_score"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/margin_10">

                            <ImageView
                                android:id="@+id/iv_result_score"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                app:srcCompat="@drawable/ic_result_score"
                                tools:ignore="ContentDescription" />

                            <LinearLayout
                                android:id="@+id/ll_result_score"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/margin_10"
                                android:layout_toRightOf="@+id/iv_result_score"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/youScored"
                                    android:textSize="@dimen/text_size_14" />

                                <TextView
                                    android:id="@+id/tv_result_score"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/md_black_1000"
                                    android:textSize="@dimen/text_size_14"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </RelativeLayout>

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/spacing_5dp"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:layout_marginRight="@dimen/margin_10"
                            android:layerType="software"
                            android:src="@drawable/dotted_drawable"
                            tools:ignore="ContentDescription" />

                        <RelativeLayout
                            android:id="@+id/rl_result_rank"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/margin_10">

                            <ImageView
                                android:id="@+id/iv_result_rank"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                app:srcCompat="@drawable/ic_result_rank"
                                tools:ignore="ContentDescription" />

                            <LinearLayout
                                android:id="@+id/ll_result_rank"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/margin_10"
                                android:layout_toRightOf="@+id/iv_result_rank"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/yourRank"
                                    android:textSize="@dimen/text_size_14" />

                                <TextView
                                    android:id="@+id/tv_result_rank"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/md_black_1000"
                                    android:textSize="@dimen/text_size_14"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <RelativeLayout
                                android:id="@+id/rl_refresh_rank"
                                android:layout_width="@dimen/spacing_30dp"
                                android:layout_height="@dimen/spacing_30dp"
                                android:visibility="visible"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:background="@drawable/result_refresh_border">

                                <ImageView
                                    android:id="@+id/iv_result_refresh"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    app:srcCompat="@drawable/ic_refresh_result"
                                    tools:ignore="ContentDescription" />
                            </RelativeLayout>
                            <ProgressBar
                                android:id="@+id/progressBar_refresh"
                                android:layout_width="@dimen/spacing_30dp"
                                android:layout_height="@dimen/spacing_30dp"
                                android:visibility="gone"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"/>

                        </RelativeLayout>

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/spacing_5dp"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:layout_marginRight="@dimen/margin_10"
                            android:layerType="software"
                            android:src="@drawable/dotted_drawable"
                            tools:ignore="ContentDescription" />

                        <RelativeLayout
                            android:id="@+id/rl_result_time"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/margin_10">

                            <ImageView
                                android:id="@+id/iv_result_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                app:srcCompat="@drawable/ic_result_time"
                                tools:ignore="ContentDescription" />

                            <LinearLayout
                                android:id="@+id/ll_result_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/margin_10"
                                android:layout_toRightOf="@+id/iv_result_time"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/timeSpent"
                                    android:textSize="@dimen/text_size_14" />

                                <TextView
                                    android:id="@+id/tv_result_time"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/md_black_1000"
                                    android:textSize="@dimen/text_size_14"
                                    android:textStyle="bold" />
                            </LinearLayout>
                        </RelativeLayout>
                    </LinearLayout>
                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/cardView_top2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cardView_top1"
                android:layout_marginLeft="@dimen/margin_10"
                android:layout_marginRight="@dimen/margin_10"
                android:background="@color/white"
                app:cardCornerRadius="@dimen/spacing_5dp"
                app:cardElevation="3dp"
                app:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_10"
                    android:layout_marginRight="@dimen/margin_10">

                    <RelativeLayout
                        android:id="@+id/rl_result_analysis"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <View
                            android:id="@+id/view1"
                            android:layout_width="@dimen/spacing_15dp"
                            android:layout_height="@dimen/spacing_25dp"
                            android:background="@color/md_deep_orange_700" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:layout_toRightOf="@+id/view1"
                            android:text="@string/overAllAnalysis"
                            android:textColor="@color/md_black_1000"
                            android:textSize="@dimen/text_size_16"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tv_result_share"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="@string/shareUrResult"
                            android:textAllCaps="true"
                            android:visibility="gone"
                            android:textColor="@color/callColor"
                            android:textSize="@dimen/text_size_14" />
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/rl_overall_mark"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@+id/rl_result_analysis"
                        android:layout_marginTop="@dimen/spacing_5dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/spacing_5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_correct_mark"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:textColor="@color/answeredColor"
                                android:textSize="@dimen/text_size_16"
                                android:textStyle="normal" />

                            <ProgressBar
                                android:id="@+id/progressBar_correct"
                                style="@android:style/Widget.ProgressBar.Horizontal"
                                android:layout_width="@dimen/spacing_30dp"
                                android:layout_height="@dimen/spacing_120dp"
                                android:layout_below="@+id/tv_correct_mark"
                                android:layout_centerHorizontal="true"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/spacing_5dp"
                                android:max="100"
                                android:progressDrawable="@drawable/progress_drawable_correct" />

                            <TextView
                                android:id="@+id/tv_correct_mark1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/progressBar_correct"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="@dimen/spacing_5dp"
                                android:text="Correct"
                                android:textColor="@color/answeredColor"
                                android:textSize="@dimen/text_size_16"
                                android:textStyle="normal" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/spacing_5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_wrong_mark"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:gravity="center"
                                android:textColor="@color/notAnsweredColor"
                                android:textSize="@dimen/text_size_16"
                                android:textStyle="normal" />

                            <ProgressBar
                                android:id="@+id/progressBar_wrong"
                                style="@android:style/Widget.ProgressBar.Horizontal"
                                android:layout_width="@dimen/spacing_30dp"
                                android:layout_height="@dimen/spacing_120dp"
                                android:layout_below="@+id/tv_wrong_mark"
                                android:layout_centerHorizontal="true"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/spacing_5dp"
                                android:max="100"
                                android:progressDrawable="@drawable/progress_drawable_wrong" />

                            <TextView
                                android:id="@+id/tv_wrong_mark1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/progressBar_wrong"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="@dimen/spacing_5dp"
                                android:text="Wrong"
                                android:textColor="@color/notAnsweredColor"
                                android:textSize="@dimen/text_size_16"
                                android:textStyle="normal" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/spacing_5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_skip_mark"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:textColor="@color/skipColor"
                                android:textSize="@dimen/text_size_16"
                                android:textStyle="normal" />

                            <ProgressBar
                                android:id="@+id/progressBar_skip"
                                style="@android:style/Widget.ProgressBar.Horizontal"
                                android:layout_width="@dimen/spacing_30dp"
                                android:layout_height="@dimen/spacing_120dp"
                                android:layout_below="@+id/tv_skip_mark"
                                android:layout_centerHorizontal="true"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/spacing_5dp"
                                android:max="100"
                                android:progressDrawable="@drawable/progress_drawable_skip" />

                            <TextView
                                android:id="@+id/tv_skip_mark1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/progressBar_skip"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="@dimen/spacing_5dp"
                                android:text="Skipped"
                                android:textColor="@color/skipColor"
                                android:textSize="@dimen/text_size_16"
                                android:textStyle="normal" />

                        </RelativeLayout>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rl_overall_mark"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/margin_10"
                        android:background="@color/white"
                        android:gravity="center"
                        android:layout_marginBottom="@dimen/margin_10"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_again_attempt"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/spacing_40dp"
                            android:layout_gravity="end"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:layout_weight="1"
                            android:background="@drawable/button_see_ans_selector"
                            android:backgroundTint="@color/callColor"
                            android:text="@string/attemptAgain"
                            android:textAllCaps="true"
                            android:visibility="gone"
                            android:textColor="@color/callColor"
                            android:textSize="@dimen/text_size_14" />

                        <Button
                            android:id="@+id/btn_view_solution"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/spacing_40dp"
                            android:padding="@dimen/margin_10"
                            android:layout_marginLeft="@dimen/spacing_15dp"
                            android:layout_weight="1"
                            android:background="@drawable/back_gradient"
                            android:backgroundTint="@color/callColor"
                            android:text="@string/viewSolution"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="@dimen/text_size_14"
                            android:visibility="visible" />

                    </LinearLayout>
                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/cardView_top3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cardView_top2"
                android:layout_marginLeft="@dimen/margin_10"
                android:layout_marginRight="@dimen/margin_10"
                android:background="@color/white"
                app:cardCornerRadius="@dimen/spacing_5dp"
                app:cardElevation="3dp"
                app:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_10">

                    <RelativeLayout
                        android:id="@+id/rl_result_section"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <View
                            android:id="@+id/view2"
                            android:layout_width="@dimen/spacing_15dp"
                            android:layout_height="@dimen/spacing_25dp"
                            android:background="@color/md_deep_orange_700" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:layout_toRightOf="@+id/view2"
                            android:text="@string/sectionAnalysis"
                            android:textColor="@color/md_black_1000"
                            android:textSize="@dimen/text_size_16"
                            android:textStyle="bold" />
                    </RelativeLayout>

                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/rv_section_topic"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rl_result_section"
                        android:layout_marginTop="@dimen/margin_10"
                        android:background="@color/backColor" />

                    <RelativeLayout
                        android:layout_below="@+id/rv_section_topic"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/rv_section_rank"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <RelativeLayout
                            android:layout_centerVertical="true"
                            android:id="@+id/ll_section_rank_top"
                            android:layout_toRightOf="@+id/rv_section_rank"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:id="@+id/ll_section_rank"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/spacing_5dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/tv_you_section_mark"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerHorizontal="true"
                                        android:textColor="@color/md_black_1000"
                                        android:textSize="@dimen/text_size_16"
                                        android:textStyle="normal" />

                                    <ProgressBar
                                        android:id="@+id/progressBar_section_you"
                                        style="@android:style/Widget.ProgressBar.Horizontal"
                                        android:layout_width="@dimen/spacing_30dp"
                                        android:layout_height="@dimen/spacing_120dp"
                                        android:layout_below="@+id/tv_you_section_mark"
                                        android:layout_centerHorizontal="true"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/spacing_5dp"
                                        android:max="100"
                                        android:progressDrawable="@drawable/progress_drawable_correct" />

                                    <TextView
                                        android:id="@+id/tv_correct_section_mark1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@+id/progressBar_section_you"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="@dimen/spacing_5dp"
                                        android:text="You"
                                        android:textColor="@color/md_black_1000"
                                        android:textSize="@dimen/text_size_16"
                                        android:textStyle="normal" />

                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/spacing_5dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/tv_topper_section_mark"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerHorizontal="true"
                                        android:gravity="center"
                                        android:textColor="@color/md_black_1000"
                                        android:textSize="@dimen/text_size_16"
                                        android:textStyle="normal" />

                                    <ProgressBar
                                        android:id="@+id/progressBar_section_topper"
                                        style="@android:style/Widget.ProgressBar.Horizontal"
                                        android:layout_width="@dimen/spacing_30dp"
                                        android:layout_height="@dimen/spacing_120dp"
                                        android:layout_below="@+id/tv_topper_section_mark"
                                        android:layout_centerHorizontal="true"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/spacing_5dp"
                                        android:max="100"
                                        android:progressDrawable="@drawable/progress_drawable_wrong" />

                                    <TextView
                                        android:id="@+id/tv_wrong_section_mark1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@+id/progressBar_section_topper"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="@dimen/spacing_5dp"
                                        android:text="Topper"
                                        android:textColor="@color/md_black_1000"
                                        android:textSize="@dimen/text_size_16"
                                        android:textStyle="normal" />

                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/spacing_5dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/tv_average_section_mark"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerHorizontal="true"
                                        android:textColor="@color/md_black_1000"
                                        android:textSize="@dimen/text_size_16"
                                        android:textStyle="normal" />

                                    <ProgressBar
                                        android:id="@+id/progressBar_section_average"
                                        style="@android:style/Widget.ProgressBar.Horizontal"
                                        android:layout_width="@dimen/spacing_30dp"
                                        android:layout_height="@dimen/spacing_120dp"
                                        android:layout_below="@+id/tv_average_section_mark"
                                        android:layout_centerHorizontal="true"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/spacing_5dp"
                                        android:max="100"
                                        android:progressDrawable="@drawable/progress_drawable_skip" />

                                    <TextView
                                        android:id="@+id/tv_skip_section_mark1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@+id/progressBar_section_average"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="@dimen/spacing_5dp"
                                        android:text="Average"
                                        android:textColor="@color/md_black_1000"
                                        android:textSize="@dimen/text_size_16"
                                        android:textStyle="normal" />
                                </RelativeLayout>

                            </LinearLayout>
                            <Button
                                android:id="@+id/btn_section_view_solution"
                                android:layout_marginTop="@dimen/spacing_20dp"
                                android:layout_marginRight="@dimen/spacing_20dp"
                                android:layout_marginBottom="@dimen/margin_10"
                                android:padding="@dimen/margin_10"
                                android:layout_centerHorizontal="true"
                                android:layout_below="@+id/ll_section_rank"
                                android:layout_width="wrap_content"
                                android:layout_centerVertical="true"
                                android:gravity="center"
                                android:visibility="gone"
                                android:layout_height="@dimen/spacing_40dp"
                                android:background="@drawable/button_see_ans_selector"
                                android:backgroundTint="@color/callColor"
                                android:text="@string/viewSolution"
                                android:textAllCaps="true"
                                android:textColor="@color/callColor"
                                android:textSize="@dimen/text_size_14" />
                        </RelativeLayout>

                    </RelativeLayout>
                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/cardView_top4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cardView_top3"
                android:layout_marginLeft="@dimen/margin_10"
                android:layout_marginRight="@dimen/margin_10"
                android:background="@color/white"
                app:cardCornerRadius="@dimen/spacing_5dp"
                app:cardElevation="3dp"
                app:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_10"
                    android:layout_marginRight="@dimen/margin_10">

                    <RelativeLayout
                        android:id="@+id/rl_result_rackers"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <View
                            android:id="@+id/view3"
                            android:layout_width="@dimen/spacing_15dp"
                            android:layout_height="@dimen/spacing_25dp"
                            android:background="@color/md_deep_orange_700" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_10"
                            android:layout_toRightOf="@+id/view3"
                            android:text="@string/topRankers"
                            android:textColor="@color/md_black_1000"
                            android:textSize="@dimen/text_size_16"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tv_view_top_ranker"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/margin_10"
                            android:text="@string/viewAll"
                            android:textAllCaps="true"
                            android:textColor="@color/callColor"
                            android:textSize="@dimen/text_size_14" />
                    </RelativeLayout>

                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/rv_top_rankers"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rl_result_rackers" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                android:id="@+id/cardView_top5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cardView_top4"
                android:layout_marginLeft="@dimen/margin_10"
                android:layout_marginRight="@dimen/margin_10"
                android:background="@color/white"
                app:cardCornerRadius="@dimen/spacing_5dp"
                app:cardElevation="3dp"
                android:visibility="gone"
                app:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_10">

                    <TextView
                        android:id="@+id/tv_feed_back_msg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/feedBackMsg"
                        android:textSize="@dimen/text_size_14"/>

                    <TextView
                        android:layout_marginTop="@dimen/margin_10"
                        android:background="@color/backColor"
                        android:id="@+id/tv_feed_back_rate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_feed_back_msg"
                        android:padding="@dimen/margin_10"
                        android:text="@string/rateMsg"
                        android:textColor="@color/md_black_1000"
                        android:textSize="@dimen/text_size_18"
                        android:textStyle="bold" />
                    <RatingBar
                        android:layout_marginTop="@dimen/margin_10"
                        android:layout_below="@+id/tv_feed_back_rate"
                        android:id="@+id/ratingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:numStars="5"
                        android:progressBackgroundTint="@color/backColor"
                        android:progressTint="@color/md_deep_orange_700"
                        android:secondaryProgressTint="@color/backColor"
                        android:layout_centerHorizontal="true"
                        android:stepSize="1.0"/>
                    <TextView
                        android:id="@+id/tv_send_rate_us"
                        android:layout_below="@+id/ratingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="@color/white"
                        android:text="@string/send"
                        android:visibility="gone"
                        android:background="@color/callColor"
                        android:paddingLeft="@dimen/margin_10"
                        android:paddingRight="@dimen/margin_10"
                        android:paddingBottom="@dimen/spacing_5dp"
                        android:paddingTop="@dimen/spacing_5dp"
                        android:layout_marginBottom="@dimen/spacing_20dp"
                        android:layout_marginRight="@dimen/margin_10"
                        android:textSize="@dimen/text_size_14"/>

                </RelativeLayout>


            </android.support.v7.widget.CardView>

        </RelativeLayout>
    </android.support.v4.widget.NestedScrollView>
</RelativeLayout>